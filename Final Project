void accountInfo()
{
	printf("This is where you would get account information \n");
	printf("Customer account holder information is unavailable at this time \n");
	system("pause");
}
void transInfo()
{
	printf("This is where you would get transaction information \n");
	printf("There is no transaction history availabe at this time \n");
	system("pause");
}

void logoBranding()
{
	system("cls");
	system("COLOR 07");

	printf("\n");
	printf("\n");
	printf("  _____   ________   _____    ___          \n");
	printf(" | ____| |___  ___| | ___ | ||___||        \n");
