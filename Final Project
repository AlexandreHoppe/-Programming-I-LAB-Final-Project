	printf(" |_____|     ||     ||   || ||    ||         \n");

	printf("\n");
}

int closeProgram()
{
	char yesNo;

	printf("Are you sure you want to exit <Y/N>: ");

	scanf_s("%s", &yesNo); //ask an answer to 8

	printf("\n"); //separate the answer from the result

	if (yesNo == 'Y' || yesNo == 'y') { //order for Y
		closingScreen();
		system("pause");
		return 0;
	}
	if (yesNo == 'N' || yesNo == 'n') { //order for N
		screenMenu(); //return to menu
	}

}

int blockAccount()
{
	char yesNoTwo;
	char yesNoThree;

	printf("This feature will block your account.\n\n Do you want to block your account? <Y/N>  ");
	scanf_s("%s", &yesNoTwo);

	if (yesNoTwo == 'Y' || yesNoTwo == 'y') {
		printf("\n\n");
		printf("Star Bank is not responsible for an blocked account. \n\n Are you sure? <Y/N>  ");
		scanf_s("%s", &yesNoThree);
		printf("\n\n");

		if (yesNoThree == 'y' || yesNoThree == 'y') {
			printf("Your account is now blocked. To have more information, contact your local bank\n");
			system("pause");
			blockedAccount();
		}

		if (yesNoThree == 'N' || yesNoThree == 'n') {

			blockAccount();
			return 0;
		}
	}
	if (yesNoTwo == 'N' || yesNoTwo == 'n') {
		screenMenu();
	}
}

int blockedAccount()
{
	int numMenu;



	system("cls");
	printf("1. Create New Account \n"); // output menu
	printf("2. Cash Deposit \n");
	printf("3. Cash Withdrawal \n");
	printf("4. Fund Transfer \n");
	printf("5. Account Information \n");
	printf("6. Transaction Information \n");
	printf("7. Block Account.\n");
	printf("8. Exit \n");
	printf(" \n");
	printf(" \n");
	printf("Press a choice between the range [1-8] "); //ask a number
	scanf_s("%d", &numMenu); //input the number
	printf("\n");

	while (numMenu != 8) {
		printf("This account is close. Contact your local bamk to receive more information\n");
		system("pause");

		system("cls");
		printf("1. Create New Account \n"); // output menu
		printf("2. Cash Deposit \n");
		printf("3. Cash Withdrawal \n");
		printf("4. Fund Transfer \n");
		printf("5. Account Information \n");
		printf("6. Transaction Information \n");
		printf("7. Block Account.\n");
		printf("8. Exit \n");
		printf(" \n");
		printf(" \n");
		printf("Press a choice between the range [1-8] "); //ask a number
		scanf_s("%d", &numMenu); //input the number
		printf("\n");
	}

	closingScreen();
	system("pause");

}

